# Anbernic devices - all share the same USB adapter so end up with the same
# reserved IP. So use HostkeyAlias to differentiate in known_hosts.

Host 351elec
  HostkeyAlias 351elec
  Hostname 192.168.0.231
  User root

Host arkOS
  HostkeyAlias arkOS
  Hostname 192.168.0.231
  User ark

Host 350adam
  HostkeyAlias 350adam
  Hostname 192.168.0.231
  User od

Host 350beta
  HostkeyAlias 350beta
  Hostname 192.168.0.231
  User od

Host 350mUSB
  Hostname 10.1.1.2
  User root
