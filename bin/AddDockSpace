#!/bin/bash
#
# SF: From https://github.com/danro/dotfiles/blob/master/scripts/dockspace
#
# Adds a blank space to the dock

if [ $(uname) != 'Darwin' ]; then
  echo "$(basename $0) is only applicable on macOS"; exit 1
fi

defaults write com.apple.dock persistent-apps -array-add '{"tile-type"="spacer-tile";}'
killall Dock
