# Set path to point to Homebrew before standard paths
export PATH=/usr/local/bin:/usr/local/sbin:~/bin:~/local/bin:$PATH

# Add RVM to PATH for scripting
export PATH=$PATH:$HOME/.rvm/bin

# Add RealVNC paths
export PATH=$HOME/Code/WorkProjects/builds:$PATH
export PATH=$HOME/Code/WorkProjects/tools:$PATH

# Add scripts path
export PATH=$HOME/Code/TerminalUtils/dotfiles/scripts:$PATH

# Temporary: Add cmake 3.0 and Emscripten paths
export PATH="$HOME/Code/WorkProjects/emscripten-tools/emsdk-portable/emscripten/master:$PATH"
export PATH="$HOME/Code/WorkProjects/emscripten-tools/emsdk-portable/clang/fastcomp/build_master_64/bin:$PATH"
export PATH="$HOME/Code/WorkProjects/emscripten-tools/cmake-3.0.0-Linux-i386/bin:$PATH"

