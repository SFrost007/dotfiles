# Set path to point to Homebrew before standard paths
export PATH=/usr/local/bin:/usr/local/sbin:~/bin:~/local/bin:$PATH

# Add RVM to PATH for scripting
export PATH=$PATH:$HOME/.rvm/bin

# Add RealVNC paths
export PATH=$HOME/Code/WorkProjects/builds:$PATH
export PATH=$HOME/Code/WorkProjects/tools:$PATH
